package collection;

import java.util.Vector;

public class CollectionNumber {
public static void main(String[] args) {
	
	
	Vector v=new Vector();
	
		v.add(1);
		v.add(2);
		v.add(3);
		v.add("usha");
		v.add("ashok");
		v.add(4);
		System.out.println(v);
		
		v.remove(3);
		System.out.println(v);
		
		for(int i1 =0; i1< v.size();i1++)
	{
			System.out.print(v.get(i1)+ " ");
	}	
}
}
