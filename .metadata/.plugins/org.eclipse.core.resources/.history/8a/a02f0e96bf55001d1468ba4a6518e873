package finalassegnment;

public class Main3 {
public static void main(String[] args) {
	String String1="USHA";
	String String2="AHSU";
	if(String1.equalsIgnoreCase(String2)) {
		System.out.println("string1 and string2 are Anagrams");
	}
	else {
		System.out.println("Strings are not anagrams");
	}
}
}
