package finalassegnment;

public class Main2 {
//	static String String1="SHORE";
//	static String String2="HORSE";
public static void main(String[] args) {
	String String1="SHORE";
	String String2="HORSE";
	if(String1.endsWith(String2)) {
		System.out.println("are anagrams");
	}
	else {
		System.out.println("not a Anagrams");
	}
}
}
